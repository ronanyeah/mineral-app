    "@types/react-dom": "^18.3.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "css-loader": "^7.1.2",
    "elm": "^0.19.1-3",
    "elm-webpack-loader": "^8.0.0",
    "numbro": "^2.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "style-loader": "^4.0.0",
